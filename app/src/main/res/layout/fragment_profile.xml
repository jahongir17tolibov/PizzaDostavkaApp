            android:layout_height="wrap_content"
            app:cornerRadius="0dp"
            app:strokeWidth="0dp"
            android:elevation="7dp">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:padding="8dp">

                <ImageView
